<!DOCTYPE html>
<html>
<head>
    <title></title>
    @model IEnumerable<EndedTask.Models.User>
    @addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
</head>
<body>
    @await Html.PartialAsync("NavBar");
    <div class="container">
        <h2 style="padding-top: 130px; font-weight: 700; display: flex; justify-content: center;" class="intro__subtitle">Список пользователей</h2>
        <div style="align-items: normal" class="container__select">
            <table class="table">
                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.Email</td>
                        <td>
                            <a class="btn btn--red" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
                        </td>
                    </tr>
                }
            </table>
            </div>
        </div>

</body>
</html>